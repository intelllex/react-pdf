  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "babel-jest": "^26.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "nwb": "0.24.x",
    "nwb-sass": "^0.10.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "author": "trinhngocdieu",
  "homepage": "https://intelllex.github.io/react-pdf/",
